{
	"author": "Jonathan Dahan <jonathan@jonathan.is>",
	"name": "pigpio.js",
	"description": "A node.js library for the pigpio daemon, allowing fast, powerful PWM control of the Raspberry Pi pins",
	"version": "0.0.1",
	"main": "lib/pigpio",
	"scripts": {
    "build": "coffee --bare --compile --output lib/pigpio src/pigpio/*.coffee",
    "prepublish": "coffee --bare --compile --output lib/pigpio src/pigpio/*.coffee",
    "postpublish": "rm -rf lib"
  },
	"repository": {
		"type": "git",
		"url": "git@github.com:jedahan/pigpio.js.git"
	},
	"homepage": "https://github.com/jedahan/pigpio.js",
	"bugs": {
		"url": "https://github.com/sarfata/pigpio.js/issues"
	},
	"licenses": [
    { "type": "MIT",
      "url": "http://github.com/jedahan/pigpio.js/blob/master/MIT-LICENSE"
    }
  ]
	"tags": ["raspberry", "pi", "pwm", "gpio"]
}
